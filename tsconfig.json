{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2022",
    "noImplicitAny": false,
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "rootDir": "src",
    "outDir": "",
    "declaration": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "removeComments": true
  },
  "exclude": [
    "node_modules",
    "*.d.ts"
  ]
}